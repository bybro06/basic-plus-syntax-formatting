{
  "name": "Basic Plus Theme Light",
  "type": "light",
  "colors": {
    "editor.background": "#EEEEEE",
    "editor.foreground": "#333333"
  },
  "tokenColors": [
    {
      "scope": "keyword.control.bp",
      "settings": {
        "foreground": "#7777FF"
      }
    },
    {
      "scope": "string.quoted.double.bp",
      "settings": {
        "foreground": "#E36633"
      }
    },
    {
      "scope": "comment.block.xml",
      "settings": {
        "foreground": "#3A8020",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "entity.name.function.bp.method.before",
      "settings": {
        "foreground": "#006060"
      }
    },
    {
      "scope": "entity.name.function.bp.method.after",
      "settings": {
        "foreground": "#C24C20"
      }
    },
    {
      "scope": "entity.name.function.bp",
      "settings": {
        "foreground": "#800080"
      }
    },
    {
      "scope": "keyword.function.bp",
      "settings": {
        "foreground": "#9696FF"
      }
    },
    {
      "scope": "entity.name.subroutine.bp",
      "settings": {
        "foreground": "#800080"
      }
    },
    {
      "scope": "constant.numeric.bp",
      "settings": {
        "foreground": "#E36633"
      }
    },
    {
      "scope": "arithmetic.operator.bp",
      "settings": {
        "foreground": "#800000"
      }
    },
    {
      "scope": "entity.name.tag.xml",
      "settings": {
        "foreground": "#007ACC"
      }
    },
    {
      "scope": "entity.other.attribute-name.xml",
      "settings": {
        "foreground": "#A626A4"
      }
    },
    {
      "scope": "string.quoted.double.xml",
      "settings": {
        "foreground": "#E36633"
      }
    },
    {
      "scope": "constant.character.entity.xml",
      "settings": {
        "foreground": "#D16969"
      }
    }
  ]
}
