{
  "name": "Basic Plus Theme Dark",
  "type": "dark",
  "colors": {
    "editor.background": "#1E1E1E",
    "editor.foreground": "#D4D4D4",
    "editorLineNumber.foreground": "#858585",
    "editorCursor.foreground": "#AABBFF",
    "editor.selectionBackground": "#264F78",
    "editor.inactiveSelectionBackground": "#3A3D41"
  },
  "tokenColors": [
    {
      "scope": "keyword.control.bp",
      "settings": {
        "foreground": "#00BEEE"
      }
    },
    {
      "scope": "string.quoted.double.bp",
      "settings": {
        "foreground": "#FFAA33"
      }
    },
    {
      "scope": "comment.line.bp",
      "settings": {
        "foreground": "#44AA44",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "entity.name.function.bp.method.before",
      "settings": {
        "foreground": "#98FF98"
      }
    },
    {
      "scope": "entity.name.function.bp.method.after",
      "settings": {
        "foreground": "#FF667C"
      }
    },
    {
      "scope": "entity.name.function.bp",
      "settings": {
        "foreground": "#B07BF0"
      }
    },
    {
      "scope": "keyword.function.bp",
      "settings": {
        "foreground": "#AABBFF"
      }
    },
    {
      "scope": "entity.name.subroutine.bp",
      "settings": {
        "foreground": "#B07BF0"
      }
    },
    {
      "scope": "constant.numeric.bp",
      "settings": {
        "foreground": "#FFAA33"
      }
    },
    {
      "scope": "arithmetic.operator.bp",
      "settings": {
        "foreground": "#AABBFF"
      }
    },
    {
      "scope": "entity.name.tag.xml",
      "settings": {
        "foreground": "#569CD6"
      }
    },
    {
      "scope": "entity.other.attribute-name.xml",
      "settings": {
        "foreground": "#9CDCFE"
      }
    },
    {
      "scope": "string.quoted.double.xml",
      "settings": {
        "foreground": "#CE9178"
      }
    },
    {
      "scope": "comment.block.xml",
      "settings": {
        "foreground": "#6A9955",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "constant.character.entity.xml",
      "settings": {
        "foreground": "#DCDCAA"
      }
    }
  ]
}
